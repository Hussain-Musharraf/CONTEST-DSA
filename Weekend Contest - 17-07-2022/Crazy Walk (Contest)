class Main {
    public static void main (String[] args) {
                      // Your code here
        Scanner sc=new Scanner(System.in);
        int M=sc.nextInt();
        int N=sc.nextInt();
        if(M%2!=0 || N%2!=0){
            System.out.println("YES");
        }
        else{
            System.out.println("NO");
        }
    }
}
